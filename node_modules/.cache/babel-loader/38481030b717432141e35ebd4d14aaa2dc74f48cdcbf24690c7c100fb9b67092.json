{"ast":null,"code":"export default {\n  name: 'MealPreparingPage'\n};","map":{"version":3,"names":["name"],"sources":["src/pages/RecipePreparingPage.vue"],"sourcesContent":["<!-- <template>\r\n  <div class=\"container\">\r\n    <h1>Recipe Preparation</h1>\r\n    <div>\r\n      <b-button variant=\"primary\" @click=\"multiplyIngredients\">Multiply Ingredients</b-button>\r\n    </div>\r\n    <div v-for=\"(step, index) in recipeSteps\" :key=\"index\" class=\"step\">\r\n      <b-form-checkbox v-model=\"step.completed\">\r\n        {{ step.number }}. {{ step.step }}\r\n      </b-form-checkbox>\r\n      <ul>\r\n        <li v-for=\"ingredient in step.ingredients\" :key=\"ingredient.id\">\r\n          {{ ingredient.name }}: {{ ingredient.amount || 1 }} {{ ingredient.unit || '' }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { BButton, BFormCheckbox } from 'bootstrap-vue';\r\nimport recipeData from \"../assets/mocks/analyzedInstructions324694.json\";\r\n\r\nexport default {\r\n  name: 'MealPreparingPage',\r\n  components: {\r\n    BButton,\r\n    BFormCheckbox\r\n  },\r\n  data() {\r\n    return {\r\n      recipeSteps: [],\r\n      originalIngredients: []\r\n    };\r\n  },\r\n  mounted() {\r\n    this.loadRecipe();\r\n  },\r\n  methods: {\r\n    loadRecipe() {\r\n      console.log(recipeData); // Check if data is loaded correctly\r\n      this.recipeSteps = recipeData[0].steps.map(step => ({\r\n        ...step,\r\n        completed: false\r\n      }));\r\n      this.originalIngredients = this.recipeSteps.map(step => step.ingredients.map(ingredient => ({\r\n        ...ingredient,\r\n        originalAmount: ingredient.amount || 1\r\n      })));\r\n      console.log(this.recipeSteps); // Check if recipe steps are correctly initialized\r\n    },\r\n    multiplyIngredients() {\r\n      this.recipeSteps.forEach((step, stepIndex) => {\r\n        step.ingredients.forEach((ingredient, ingIndex) => {\r\n          ingredient.amount = (ingredient.amount || 1) * 2;\r\n        });\r\n      });\r\n      console.log(this.recipeSteps); // Verify ingredients are correctly multiplied\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  margin-top: 20px;\r\n}\r\n.step {\r\n  margin-bottom: 20px;\r\n}\r\n</style> -->\r\n<template>\r\n  <div>\r\n    <h1></h1>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    name: 'MealPreparingPage',\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>"],"mappings":"AA8EA;EACAA,IAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}